    "build-css": "node-sass-chokidar src/ -o src/",
    "build-js": "react-scripts build",
    "build-scripts": "BABEL_ENV=production webpack --progress --config webpack.scripts.js",
    "build-production": "npm-run-all lint build test-coverage",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "react-scripts test --coverage --env=jsdom --no-cache",
    "eject": "react-scripts eject",
    "lint": "tslint \"src/**/*.ts{,?}\""
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**",
      "!src/assets/images/*.svg",
      "!src/index.tsx",
      "!src/contracts/**"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 97.85,
        "statements": 97.81,
        "functions": 98,
        "branches": 95.65
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
